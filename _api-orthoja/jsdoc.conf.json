{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "exclude": ["node_modules", "logs", "database"],
        "includePattern": ".+\\.js$",
        "excludePattern": ""
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "referenceTitle": "My SDK Name",
        "disableSort": false
    },
    "opts": {
        "destination": "./documentation/",
        "readme": "./README.md",
        "encoding": "utf8",
        "private": true,
        "recurse": true
    }
}